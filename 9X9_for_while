<html>
    <head>
        <style>
            td,
            th,
            tr {
                text-align: center;
            }
            table {
                margin-left: auto;
                margin-right: auto;
            }
        </style>
    </head>
    <body>
        <div style="background-color:FFCC33;text-align:center">
            <hi>九九乘法表</hi>
        </div>
        <div style="background-color:FFFF99">
            <br/>
            <table border="4">
                <script>
                    for (var a = 1; a < 10; a++) {
                        document.write('<th>' + a + '</th>');
                    }
                    for (var a = 1; a < 10; a++) {
                        document.write('<tr>')
                        var s = 1
                        while (s < 10) {
                            document.write('<td>' + s + "X" + a + "=" + s * a + '</td>')
                            s++;
                        }
                        document.write('</tr>')
                    }
                </script>
            </table>
            <br/>
        </div>
    </body>
</html>
